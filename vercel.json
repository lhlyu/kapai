{
  "functions": {
    "api/**.go": {
      "includeFiles": "api/**"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "regions": ["hkg1"],
  "probes": [
    { "path": "/", "mustContain": "cow:go1.18.1:RANDOMNESS_PLACEHOLDER" }
  ]
}